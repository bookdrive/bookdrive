<% title "Snippets" %>
<% admin_page %>


<div class="links">
  <%= link_to 'New Snippet', new_snippet_path %>
</div>

<h1>Snippets</h1>


<%= form_tag questions_path, :method => 'get', :id => "index_search", :class => "search_form" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
  <div id="snippets"><%= render 'snippets' %></div>
<% end %>

